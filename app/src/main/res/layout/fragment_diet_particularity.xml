<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="@string/as_tu_un_r_gime_particulier"
        android:textAlignment="center"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/milk"
        android:layout_width="70dp"
        android:layout_height="70dp"
        app:layout_constraintBottom_toBottomOf="@+id/glutenfree"
        app:layout_constraintEnd_toStartOf="@+id/pork"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/glutenfree"
        app:layout_constraintTop_toTopOf="@+id/glutenfree"
        app:srcCompat="@drawable/sanslait" />

    <ImageView
        android:id="@+id/pork"
        android:layout_width="70dp"
        android:layout_height="70dp"
        app:layout_constraintBottom_toBottomOf="@+id/milk"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/milk"
        app:layout_constraintTop_toTopOf="@+id/milk"
        app:srcCompat="@drawable/sansporc" />

    <ImageView
        android:id="@+id/glutenfree"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginTop="72dp"
        app:layout_constraintEnd_toStartOf="@+id/milk"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:srcCompat="@drawable/glutenfree" />

    <ImageView
        android:id="@+id/carrot"
        android:layout_width="70dp"
        android:layout_height="70dp"
        app:layout_constraintBottom_toBottomOf="@+id/plante"
        app:layout_constraintEnd_toStartOf="@+id/pescovegetarien"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/plante"
        app:layout_constraintTop_toTopOf="@+id/plante"
        app:srcCompat="@drawable/carrot" />

    <ImageView
        android:id="@+id/plante"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginTop="48dp"
        app:layout_constraintEnd_toStartOf="@+id/carrot"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:srcCompat="@drawable/plante" />

    <ImageView
        android:id="@+id/pescovegetarien"
        android:layout_width="70dp"
        android:layout_height="70dp"
        app:layout_constraintBottom_toBottomOf="@+id/carrot"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/carrot"
        app:layout_constraintTop_toTopOf="@+id/carrot"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/pescovegetarien" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Sans gluten"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="@+id/glutenfree"
        app:layout_constraintStart_toStartOf="@+id/glutenfree"
        app:layout_constraintTop_toBottomOf="@+id/glutenfree" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Sans produit laitier"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="@+id/milk"
        app:layout_constraintStart_toStartOf="@+id/milk"
        app:layout_constraintTop_toBottomOf="@+id/milk" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Sans porc"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="@+id/pork"
        app:layout_constraintStart_toStartOf="@+id/pork"
        app:layout_constraintTop_toBottomOf="@+id/pork" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Végétalien"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="@+id/plante"
        app:layout_constraintStart_toStartOf="@+id/plante"
        app:layout_constraintTop_toBottomOf="@+id/plante" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Végétarien"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="@+id/carrot"
        app:layout_constraintStart_toStartOf="@+id/carrot"
        app:layout_constraintTop_toTopOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pescovégétarien"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/textView10"
        app:layout_constraintEnd_toEndOf="@+id/pescovegetarien"
        app:layout_constraintStart_toStartOf="@+id/pescovegetarien"
        app:layout_constraintTop_toTopOf="@+id/textView10" />


</androidx.constraintlayout.widget.ConstraintLayout>